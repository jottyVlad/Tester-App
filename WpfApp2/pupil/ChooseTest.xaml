<Window x:Class="WpfApp2.pupil.ChooseTest"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WpfApp2.pupil"
        mc:Ignorable="d"
        Title="ChooseTest" Height="450" Width="800">
    <Grid>
        <TextBlock Name="ChooseToPassage" TextAlignment="Center" FontSize="14" HorizontalAlignment="Center" Margin="0,85,0,0" TextWrapping="Wrap" Text="Выберите пакет тестов для прохождения" VerticalAlignment="Top" Height="40" Width="200"/>
        <ComboBox Name="PacketsToPassage" HorizontalAlignment="Left" Margin="296,130,0,0" VerticalAlignment="Top" Width="200" Height="25"/>
        <Button Name="PassageChoicePacket"  Content="Пройти" HorizontalAlignment="Center" Margin="296,160,296,0" VerticalAlignment="Top" Width="200" Height="27" Click="PassageChoicePacket_Click"/>

    </Grid>
</Window>
